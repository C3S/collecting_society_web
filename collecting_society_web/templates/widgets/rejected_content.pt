<span i18n:domain="collecting_society_web">
<table cellspacing=0 cellpadding=0 class="rejected-table">
    <tr data-toggle="tooltip" i18n:attributes="title"
            title="Number of files you uploaded more than once">
        <td><span i18n:translate="">Duplicates</span>:</td>
        <td>${dupl}</td>
    </tr>
    <tr data-toggle="tooltip" i18n:attributes="title"
            title="Number of files you uploaded that had invalid file formats">
        <td><span i18n:translate="">Format errors</span>:</td>
        <td>${ferrors}</td>
    </tr>
    <tr data-toggle="tooltip" i18n:attributes="title"
            title="Number of files you uploaded that where encoded with a lossy compression algorithm">
        <td><span i18n:translate="">Lossy compression</span>:</td>
        <td>${lossyc}</td>
    </tr>
    <tr data-toggle="tooltip" i18n:attributes="title"
            title="Total number of files you uploaded that where rejected">
        <td><b><span i18n:translate="">Rejected (sum)</span>:</b></td>
        <td>${rejects}</td>
    </tr>
</table>
</span>

<script>
$(document).ready(function(){
    $('[data-toggle="tooltip"]').tooltip();
});
</script>